{
  "name": "master10-webpack",
  "version": "1.0.0",
  "description": "Master10 Webpack",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "webpack-dev-server --config ./config/webpack/dev.js",
    "build": "run-p -l type-check build:prod",
    "build:prod": "npm run clean && webpack --config ./config/webpack/prod.js",
    "analyze": "npm run clean && npm run type-check && webpack --config ./config/webpack/analyze.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lemoncode/appboiler.git"
  },
  "homepage": "https://github.com/Lemoncode/appboiler/blob/main/README.md",
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "pretty-quick â€” staged",
      "git add"
    ]
  },
  "dependencies": {
    "file-loader": "^6.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^4.0.4",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^10.2.0",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
